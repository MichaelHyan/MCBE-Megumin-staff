{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.dn":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"dn_a":"query.get_equipped_item_name=='dn' && query.is_using_item && query.scoreboard('dn') > 1"},
						{"dn_b":"query.get_equipped_item_name=='dn' && query.is_using_item && query.scoreboard('dn') == 1"},
						{"dn_c":"query.get_equipped_item_name=='dn' && !query.is_using_item"},
						{"dna_a":"query.get_equipped_item_name=='dna' && query.is_using_item && query.scoreboard('dn') > 1"},
						{"dna_b":"query.get_equipped_item_name=='dna' && query.is_using_item && query.scoreboard('dn') == 1"},
						{"dna_c":"query.get_equipped_item_name=='dna' && !query.is_using_item"}
					]
				},
				"dn_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard players add @s dn -1"
					]
				},
				"dn_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/kill @n[type=!player,type=!item]",
						"/scoreboard objectives set @s dn 20"
					]
				},
				"dn_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard objectives set @s dn 20"
					]
				},
				"dna_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard players add @s dn -1"
					]
				},
				"dna_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/kill @n[rm=1,type=!item]",
						"/scoreboard objectives set @s dn 20"
					]
				},
				"dna_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/scoreboard objectives set @s dn 20"
					]
				}
			}
		}
 	}
 }